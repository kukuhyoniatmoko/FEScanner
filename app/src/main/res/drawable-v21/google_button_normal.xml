<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <size android:width="38dp"/>
            <corners
                android:bottomLeftRadius="2dp"
                android:topLeftRadius="2dp"/>
            <solid android:color="@color/text_white_primary"/>
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="42dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/blue_google"/>
            <corners
                android:bottomRightRadius="2dp"
                android:topRightRadius="2dp"/>
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/blue_google"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
</layer-list>